{
  "expo": {
    "sdkVersion": "42.0.0",
    "name": "news",
    "slug": "news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.news.simplenews",
      "buildNumber": "1.0.0",
      "permissions": ["1"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "package": "com.news.simplenews",
        "versionCode": 1
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
